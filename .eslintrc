{
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "script",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": false
    }
  },
  "extends": [
    "airbnb-base"
  ],
  "plugins": [],
  "globals": {
    "describe": true,
    "it": true
  },
  "rules": {
    "strict": 0,
    "no-param-reassign": 0,
    "class-methods-use-this": 0,
    "max-len": [
      2,
      120
    ],
    "eqeqeq": 2,
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "always"
    ]
  }
}
